<?xml version="1.0" encoding="utf-8"?>
<conf>
  <module tag="Contents" name="目录">
    <section tag="ContentsTitle" name="目录标题">
      <item tag="Fonts">
        <error-msg>
          (4.1.1)目录标题字体错误，应为黑体；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          (4.1.2)目录标题字号错误，应为小三号；
        </error-msg>
      </item>
    </section>
    <section tag="ContentsText" name="目录内容">
      <item tag="Fonts">
        <error-msg>
          (4.2.1)目录内容字体错误，应为宋体；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          (4.1.2)目录内容字号错误，应为小四号；
        </error-msg>
      </item>
      <item tag="overThree">
        <error-msg>
          (4.1.3)章节标题不应超过三级；
        </error-msg>
      </item>
      <item tag="spacingTwo">
        <error-msg>
          (4.1.4)章节标题内容与序号之间因空两格；
        </error-msg>
      </item>
    </section>
  </module>
</conf>